{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "headers": [
      {
        "source": "**/apple-app-site-association",
        "headers": [
          {
            "key": "content-type",
            "value": "application/json"
          }
        ]
      }
    ]
  }
}